{
	"name": "redesigner",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"commitmsg": "commitlint -E HUSKY_GIT_PARAMS",
		"test": "jest",
		"test:coverage": "jest --watchAll --coverage",
		"test:e2e": "playwright test",
		"prepare": "husky install",
		"prettier": "prettier \"**/*.+(js|json|ts|jsx|tsx)\"",
		"prettier-check": "prettier --check \"**/*.+(js|json|ts|jsx|tsx)\"",
		"check-format": "npm run prettier -- --list-different",
		"format": "npm run prettier -- --write",
		"audit": "npm audit --audit-level=high --omit=dev",
		"check-types": "npx --no-install tsc",
		"validate": "run-p check-types check-format"
	},
	"dependencies": {
		"@headlessui/react": "1.7.15",
		"@heroicons/react": "2.0.18",
		"@react-hook/media-query": "1.1.1",
		"@tensorflow/tfjs": "4.6.0",
		"@types/node": "20.2.5",
		"@types/react": "18.2.8",
		"@types/react-dom": "18.2.4",
		"@upstash/ratelimit": "0.4.3",
		"@upstash/redis": "1.21.0",
		"@vercel/analytics": "1.0.1",
		"autoprefixer": "10.4.14",
		"clsx": "1.2.1",
		"dotenv": "^16.3.1",
		"eslint": "8.42.0",
		"eslint-config-next": "13.4.4",
		"greenhouse-react-ui": "1.1.1",
		"next": "13.4.4",
		"nsfwjs": "2.4.2",
		"playwright": "^1.35.1",
		"postcss": "8.4.24",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-icons": "4.9.0",
		"react-loader-spinner": "^5.3.4",
		"react-uploader": "^3.0.2",
		"request-ip": "3.3.0",
		"tailwindcss": "3.3.2",
		"typescript": "5.1.3",
		"uploader": "^3.0.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.6.5",
		"@commitlint/config-conventional": "^17.6.5",
		"@playwright/test": "^1.35.1",
		"@testing-library/dom": "^9.3.1",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^14.0.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/request-ip": "0.0.37",
		"husky": "^8.0.3",
		"jest": "^29.5.0",
		"jest-environment-jsdom": "^29.5.0",
		"npm-run-all": "4.1.5",
		"prettier": "^2.8.8",
		"ts-jest": "^29.1.0"
	},
	"lint-staged": {
		"app/**/*.{js,jsx,ts,tsx}": [
			"npm run test",
			"eslint --max-warnings=0"
		]
	}
}
